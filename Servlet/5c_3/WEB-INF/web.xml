<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
             http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>HistoryDetailsServlet</servlet-name>
        <servlet-class>HistoryDetailsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HistoryDetailsServlet</servlet-name>
        <url-pattern>/HistoryDetailsServlet</url-pattern>
    </servlet-mapping>
</web-app>
